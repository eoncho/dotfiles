set showstatuslinls=2
set foucuscountent
set hintchars=asdfghjkl

map S :source ~/.vimperatorrc<CR>

map t :tabopen google 
map o :open google 

map j 5<C-e>
map k 5<C-y>

map h <C-pageup>
map l <C-pagedown>

map H :tabmove! -1<CR>
map L :tabmove! +1<CR>

map <C-p> <a-left>
map <C-n> <a-right>

echo "vimperatorrc sourced !!"
